<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".strukturOrganisasi.StrukturOrganisasiActivity">

    <ImageView
        android:id="@+id/iv_back_arrow"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="37dp"
        android:src="@drawable/ic_baseline_arrow_backblue" />

    <TextView
        android:id="@+id/tv_struktur_organisasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Struktur Organisasi"
        android:textColor="#0C159F"
        android:textSize="19sp"
        android:textStyle="bold" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_struktur_organisasi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_struktur_organisasi"
        android:layout_marginTop="66dp"
        tools:listitem="@layout/item_struktur_organisasi" />

</RelativeLayout>

<!--Sturktur Organisasii-->